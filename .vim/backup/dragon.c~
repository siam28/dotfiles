/* Prints a beautiful dragon */

#include <stdio.h>
#include <stdlib.h>

int main (int argc, char *argv[]) {

    printf("    ^  ^\n");
    printf("   ------\n");
    printf("  | _  _ |\n");
    printf("  |      |\n");
    printf("  |  --  |\n");
    printf("   ------\n");
    printf("  (=|  |=)\n");
    printf("     --\n");
    printf("     vv\n");

    return EXIT_SUCCESS;

}
